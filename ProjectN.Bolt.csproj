<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackOnBuild>true</PackOnBuild>
    <PackageId>ProjectN.Bolt</PackageId>
    <PackageVersion>2.0.0</PackageVersion>
    <Authors>ProjectN</Authors>
    <Description>Bolt .NET SDK</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/project-n-oss/bolt-sdk-net</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
      <PackageReference Include="AWSSDK.Core" Version="3.7.8.6" />
      <PackageReference Include="AWSSDK.S3" Version="3.7.8.4" />
      <PackageReference Include="AWSSDK.SecurityToken" Version="3.7.1.124" />
      <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>
</Project>
